# Worker Environment Variables
REDIS_HOST=localhost
REDIS_PORT=6379

# Meshy API (optional - leave empty to use mock provider)
# Get your API key from https://www.meshy.ai/settings/api
MESHY_API_KEY=your-meshy-api-key

# Provider selection (optional): hunyuan | meshy
PROVIDER=hunyuan

# Tencent Hunyuan (optional)
TENCENTCLOUD_SECRET_ID=your-secret-id
TENCENTCLOUD_SECRET_KEY=your-secret-key
TENCENTCLOUD_REGION=ap-guangzhou
HUNYUAN_HOST=ai3d.ap-guangzhou.tencentcloudapi.com
HUNYUAN_SERVICE=ai3d
HUNYUAN_VERSION=2025-05-13
HUNYUAN_MODE=rapid
HUNYUAN_MODEL=3.0
HUNYUAN_RESULT_FORMAT=GLB
HUNYUAN_ENABLE_PBR=true
HUNYUAN_ENABLE_GEOMETRY=false
HUNYUAN_FACE_COUNT=
HUNYUAN_GENERATE_TYPE=
HUNYUAN_POLYGON_TYPE=
HUNYUAN_POLL_INTERVAL_MS=3000
HUNYUAN_MAX_POLL_ATTEMPTS=200
# image input strategy: auto | url | base64
HUNYUAN_IMAGE_INPUT_MODE=auto
# max raw bytes when converting image URL to base64
HUNYUAN_MAX_INPUT_IMAGE_BYTES=6291456

# S3 Storage (optional - for production)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
S3_BUCKET=your-bucket-name
S3_ENDPOINT=  # For S3-compatible storage (MinIO, etc.)
S3_FORCE_PATH_STYLE=false
